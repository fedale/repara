
<thead id="" class="filter" data="">
    <tr class="">
        {{ form_start(form) }}
        {% for column in columns %}
        <th class="">
        {% if form[column.attribute] is defined %}
                {{ form_widget(form[column.attribute], {'attr': {'class': 'foo'}}) }}
        {% endif %}
        </th>
        {% endfor %}
        {{ form_widget(form.save) }}
        {{ form_end(form) }}
    </form>
    </tr>
</thead>