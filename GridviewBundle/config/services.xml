<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="gridview.filter_model" class="Fedale\GridviewBundle\Service\FilterModel">
            <argument type="service" id="form.factory"/>
            <!-- <call method="setFormFactory">
                <argument type="service" id="form.factory"/>
            </call> -->
        </service>
        <service id="Fedale\GridviewBundle\Service\FilterModelInterface" alias="gridview.filter_model"/>

        <service id="gridview.gridview" class="Fedale\GridviewBundle\Grid\Gridview">
            <argument type="service" id="twig"/>
            <call method="setFilterModel">
                <argument type="service" id="gridview.filter_model"/>
            </call>
            <!-- <call method="setFormFactory">
                <argument type="service" id="form.factory"/>
            </call> -->
        </service>

        <service id="gridview.filter_model_type" class="Fedale\GridviewBundle\Form\FilterModelType">
            <!-- <argument type="service" id="gridview.gridview" /> -->
            <argument type="service" id="gridview.filter_model" />
            <!-- <call method="setGridview">
                <argument type="service" id="gridview.gridview"/>
            </call> -->
            <!-- <call method="setFilterModel">
                <argument type="service" id="gridview.filter_model"/>
            </call> -->
            <tag name="form.type"/>
        </service>
        <service id="Fedale\GridviewBundle\Form\FilterModelType" alias="gridview.filter_model_type"/>

        <service id="twig.extension.stringloader" class="Twig\Extension\StringLoaderExtension">
            <tag name="twig.extension"/>
        </service>
        <service id="fedale.options_extension" class="Fedale\GridviewBundle\Twig\OptionsExtension">
            <tag name="twig.extension"/>
        </service>

        <service id="gridview.builder_factory" class="Fedale\GridviewBundle\Grid\GridviewBuilderFactory">
            <argument type="service" id="twig"/>
        </service>
        <service id="Fedale\GridviewBundle\Grid\GridviewBuilderFactory" alias="gridview.builder_factory"/>
                     
        <service id="Fedale\GridviewBundle\DataProvider\EntityDataProvider">
            <argument type="service" id="debug.event_dispatcher"/>
            <call method="setSort">
                <argument type="service" id="gridview.sort"/>
            </call>
            <call method="setPagination">
                <argument type="service" id="gridview.pagination"/>
            </call>
            <call method="setFilterModel">
                <argument type="service" id="gridview.filter_model"/>
            </call>
        </service>
        <service id="Fedale\GridviewBundle\DataProvider\DataProviderInterface" alias="Fedale\GridviewBundle\DataProvider\EntityDataProvider"/>

        <service id="gridview.sort" class="Fedale\GridviewBundle\Component\Sort">     
            <argument type="service" id="request_stack"/>
            <argument type="service" id="router.default"/>
        </service>
        <service id="Fedale\GridviewBundle\Component\SortInterface" alias="gridview.sort"/>
                     
        <service id="gridview.pagination" class="Fedale\GridviewBundle\Component\Pagination">
            <argument type="service" id="request_stack"/>
        </service>
        <service id="Fedale\GridviewBundle\Component\PaginationInterface" alias="gridview.pagination"/>

        
        <service id="Fedale\GridviewBundle\Grid\GridviewInterface" alias="gridview.gridview"/>

        <service id="gridview.row_subscriber" class="Fedale\GridviewBundle\EventSubscriber\RowSubscriber">
            <!-- <argument type="service" id="gridview.gridview"/> -->
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="gridview.pagination_view" class="Fedale\GridviewBundle\Component\PaginationView">
            <argument type="service" id="request_stack"/>
            <argument type="service" id="router"/>
        </service>

        <service id="gridview.pagination_extension" class="Fedale\GridviewBundle\Twig\PaginationExtension">
            <argument type="service" id="gridview.pagination_view"/>
            <tag name="twig.extension"/>
        </service>
        
       
        <!-- <service>
        </service -->
    </services>
</container>
