<table class="table table-dark">
    <tbody>
    {% for item in items %}
        <tr>
            <td>{{ item.id }}</td>
            <td>{{ item.profile.fullname }}</td>
            <td>{{ item.email }}</td>
            <td>{{ item.confirmedAt }}</td>
        </tr>
    {% else %}
        <tr>
            <td colspan="14">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>