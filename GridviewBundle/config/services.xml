<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="gridview.builder_factory" class="Fedale\GridviewBundle\Grid\GridviewBuilderFactory">
            <argument type="service" id="twig"/>
        </service>
        <service id="Fedale\GridviewBundle\Grid\GridviewBuilderFactory" alias="gridview.builder_factory"/>
                     
        <service id="Fedale\GridviewBundle\DataProvider\EntityDataProvider">
            <call method="setSort">
                <argument type="service" id="gridview.sort"/>
            </call>
            <call method="setPagination">
                <argument type="service" id="gridview.pagination"/>
            </call>
        </service>
        <service id="Fedale\GridviewBundle\DataProvider\DataProviderInterface" alias="Fedale\GridviewBundle\DataProvider\EntityDataProvider"/>

        <service id="gridview.sort" class="Fedale\GridviewBundle\Component\Sort">
            <argument type="service" id="request_stack"/>
            <argument type="service" id="router.default"/>
        </service>
        <service id="Fedale\GridviewBundle\Component\Sort" alias="gridview.sort"/>
        
        <service id="gridview.pagination" class="Fedale\GridviewBundle\Component\Pagination">
            <argument type="service" id="request_stack"/>
        </service>
        <service id="Fedale\GridviewBundle\Component\Pagination" alias="gridview.pagination"/>
    </services>
</container>
