<tbody>
{% for model in models %}
    <tr>
        {% for column in columns %}
            <td>
            {# {{ model[column.label] }} #}
            {# {{ column.content }} #}
            {{ attribute(column, 'render', [loop.index, model]) }}
            </td>
        {% endfor %}
    </tr>
{% else %}
    <tr>
        <td colspan="14">no records found</td>
    </tr>
{% endfor %}
</tbody>