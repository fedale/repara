<tbody>
{% for entity in entities %}
    <tr>
        {% for column in columns %}
            <td>
                {% if not column.value|default(false) %}
                    {{ attribute(entity, column.property) }}
                {% else %}
                    {{ column.value|default(null) }}
                {% endif %}
            </td>
        {% endfor %}
    </tr>
{% else %}
    <tr>
        <td colspan="14">no records found</td>
    </tr>
{% endfor %}
</tbody>